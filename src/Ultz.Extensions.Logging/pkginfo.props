<Project>
    <PropertyGroup>
        <Description>
            A Microsoft.Extensions.Logging.Abstractions implementation, only with more customaization and less faff.
        </Description>
        <Version>4.0.0</Version>
        <LangVersion>9</LangVersion>
        <PackageReleaseNotes>
            Everything is now controled by the logger provider. The UltzLogger type is now internal, and
            IUltzLoggerObject removed.
        </PackageReleaseNotes>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/Ultz/Bcl</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
    </PropertyGroup>
</Project>